<%- include('header') %>

          <div class="container mt-4" style="min-height:600px">
              <h2>Teacher Page</h2>
              <div class="row">
                  <div class="col-md-12">
                      <table class="table table-dark table-bordered table-hover table-striped">
                          <tr>
                              <th>S.No.</th>
                              <th>Name</th>
                              <th>Salary</th>
                              <th>Age</th>
                              <th>Gender</th>
                              <th>Subject</th>
                          </tr>
                          
                          <% info.forEach((x, n)=>{ %>
                            <tr>
                                <td><%= n+1 %></td>
                                <td><%= x.name %></td>
                                <td><%= x.salary %></td>
                                <td><%= x.age %></td>
                                <td><%= x.gender %></td>
                                <td><%= x.subject %></td>
                                
                            </tr>


                          <% }) %>
                          
                      </table>

                  </div>
              </div>
          </div>
<%- include('footer') %>