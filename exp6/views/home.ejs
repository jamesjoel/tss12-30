<%- include('header') %>

          <div class="container mt-4" style="min-height:600px">
              <h2>Home Page</h2>
              <div class="row">
                  <div class="col-md-12">
                      <table class="table table-dark table-hover table-striped">
                          <tr>
                              <th>S.No.</th>
                              <th>Title</th>
                              <th>Price</th>
                              <th>Image</th>
                              <th>Category</th>
                          </tr>
                          <% product.forEach((x, n)=>{ %>
                                <tr>
                                    <td><%= n+1 %></td>
                                    <td><%= x.title %></td>
                                    <td>&#8377; <%= x.price %></td>
                                    <td><img src="<%= x.image %>" height="80" width="80" /></td>
                                    <td><%= x.category %></td>
                                </tr>
                          <% }) %>
                      </table>
                  </div>
              </div>
          </div>
<%- include('footer') %>