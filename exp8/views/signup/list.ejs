<%- include('../includes/header') %>

          <div class="container mt-4" style="min-height:600px">
              <div class="row">

                <!-- <a href="/signup/demo/indore/25/rohit">OK</a> -->

                <div class="col-md-8 offset-md-2">
                    <h3 class="text-center">All Student List</h3>
                    <table class="table table-dark table-hover table-striped table-bordered">
                        <tr>
                            <th>S.No.</th>
                            <th>Name</th>
                            <th>More</th>
                            <th>Delete</th>
                            
                        </tr>
                        <% result.forEach((x, n)=>{ %>
                            <tr>
                                <td><%= n+1 %></td>
                                <td><%= x.fullname %></td>
                                <td><a href="/signup/more/<%= x._id %>" class="btn btn-sm btn-info">More</a></td>
                                <td><a href="/signup/delete/<%= x._id %>" class="btn btn-sm btn-danger">Delete</a></td>
                            </tr>
                        <% }) %>

                    </table>
                </div>    
              </div>
          </div>
<%- include('../includes/footer') %>