<%- include('../includes/header') %>

          <div class="container mt-4" style="min-height:600px">
              <div class="row">
                <div class="col-md-8 offset-md-2">
                    <h3 class="text-center">All Student List</h3>
                    <table class="table table-dark table-hover table-striped table-bordered">
                        <tr>
                            <th>S.No.</th>
                            <th>Name</th>
                            <th>Age</th>
                            <th>Fee</th>
                            <th>Address</th>
                        </tr>
                        <% result.forEach((x, n)=>{ %>
                            <tr>
                                <td><%= n+1 %></td>
                                <td><%= x.fullname %></td>
                                <td><%= x.age %></td>
                                <td><%= x.fee %></td>
                                <td><%= x.address %></td>
                            </tr>
                        <% }) %>

                    </table>
                </div>    
              </div>
          </div>
<%- include('../includes/footer') %>